class Stacks{
    class stack{
        public int aa;
        public int ll;
        public int xi;
        public int yi;
        public int color;
    }
    public stack[] data;
    public int Top;
    public int Down;
    public Stacks(int n){
        int x=(new Double(Math.log(n))).intValue();
        data=new stack[x];
        Top=0;
        Down=0;
    }
    //ya
    public void push(int a,int l,int  x,int y){
        Top++;
        data[Top].aa=a;
        data[Top].ll=l;
        data[Top].xi=x;
        data[Top].yi=y;

    }
}
